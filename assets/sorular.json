  "quizzes": [
    {
      "id": "kurtulus_savasi_muharebeler_donemi_3",
      "baslik": "Kurtuluş Savaşı Muharebeler Dönemi-3",
      "kategoriId": "tarih",
      "konuId": "kurtulus_savasi_muharebeler_donemi",
      "isNew": false,
      "soruSayisi": 20,
      "sureDakika": 20,
      "sorular": [
        {
          "soruMetni": "Mudanya Ateşkes görüşmelerine hangi devlet, doğrudan katılmayıp, görüşmeleri bir gemiden takip etmiş ve antlaşmayı imzalamak zorunda kalmıştır?",
          "dogruCevapIndex": 3,
          "secenekler": [
            "İngiltere",
            "Fransa",
            "İtalya",
            "Yunanistan"
          ]
        },
        {
          "soruMetni": "Lozan Konferansı'na İtilaf Devletleri'nin hem TBMM Hükümeti'ni hem de İstanbul Hükümeti'ni birlikte davet etmesi, aşağıdaki hangi önemli iç siyasi gelişmeye yol açmıştır?",
          "dogruCevapIndex": 1,
          "secenekler": [
            "Cumhuriyetin ilan edilmesi",
            "Saltanatın kaldırılması",
            "Başkomutanlık Kanunu'nun süresiz uzatılması",
            "Ankara'nın başkent ilan edilmesi"
          ]
        },
        {
          "soruMetni": "Sakarya Meydan Muharebesi, Türk tarihi açısından 1683 yılındaki hangi olaydan beri süregelen geri çekilişi durduran \"savunma\" savaşı olarak kabul edilir?",
          "dogruCevapIndex": 1,
          "secenekler": [
            "I. Viyana Kuşatması",
            "II. Viyana Kuşatması",
            "Prut Antlaşması",
            "Karlofça Antlaşması"
          ]
        },
        {
          "soruMetni": "Mudanya Ateşkes Antlaşması'nın, İstanbul ve Boğazlar üzerindeki idari yetkinin TBMM Hükümeti'ne bırakılmasını öngörmesi; \nI. Osmanlı Devleti'nin hukuken sona ermesi,\nII. İtilaf Devletleri arasındaki görüş birliğinin bozulması,\nIII. Ankara'nın başkent ilan edilmesi\ndurumlarından hangilerine kanıt oluşturur?",
          "dogruCevapIndex": 0,
          "secenekler": [
            "Yalnız I",
            "Yalnız III",
            "I ve II",
            "I, II ve III"
          ]
        },
        {
          "soruMetni": "I. İnönü Muharebesi'nin kazanılmasında, aynı savaş sırasında bastırılan hangi iç isyanın da rolü büyük olmuştur?",
          "dogruCevapIndex": 2,
          "secenekler": [
            "Koçgiri İsyanı",
            "Demirci Mehmet Efe İsyanı",
            "Çerkez Ethem İsyanı",
            "Delibaş Mehmet İsyanı"
          ]
        },
        {
          "soruMetni": "TBMM Hükümeti'ni ve Misak-ı Millî'yi tanıyan ilk İtilaf devleti hangisidir?",
          "dogruCevapIndex": 2,
          "secenekler": [
            "İtalya",
            "İngiltere",
            "Fransa",
            "Sovyet Rusya"
          ]
        },
        {
          "soruMetni": "TBMM Hükümeti'ni tanıyan ilk Avrupalı büyük devlet hangisidir?",
          "dogruCevapIndex": 3,
          "secenekler": [
            "Fransa",
            "İtalya",
            "Almanya",
            "Sovyet Rusya"
          ]
        },
        {
          "soruMetni": "Saltanatın kaldırılmasıyla; \nI. Devlet başkanı sorununun ortaya çıkması,\nII. Laikliğe giden yolda ilk adımın atılması,\nIII. İkiliğin tamamen sona ermesi\ndurumlarından hangileri gerçekleşmiştir?",
          "dogruCevapIndex": 3,
          "secenekler": [
            "Yalnız I",
            "Yalnız III",
            "I ve II",
            "I, II ve III"
          ]
        },
        {
          "soruMetni": "Güney Cephesi'nde Maraş'taki direnişin başlamasında sembolik bir rol oynayan \"Sütçü İmam Olayı\" hangi devlete karşı direnişi tetiklemiştir?",
          "dogruCevapIndex": 1,
          "secenekler": [
